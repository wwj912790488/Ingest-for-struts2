<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class
  name="com.arcsoft.commander.domain.settings.Storage" table="tbl_storages">
  <id name="id" type="java.lang.Integer">
   <column name="id"/>
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="name" type="string">
   <column name="name"/>
  </property>
  <property generated="never" lazy="false" name="path" type="string">
   <column name="path"/>
  </property>
  <property generated="never" lazy="false" name="user" type="string">
   <column name="user"/>
  </property>
  <property generated="never" lazy="false" name="pwd" type="string">
   <column name="password"/>
  </property>
  <property generated="never" lazy="false" name="type" type="string">
   <column name="type"/>
  </property>
 </class>
</hibernate-mapping>
