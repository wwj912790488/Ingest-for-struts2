<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.arcsoft.commander.domain.record.ScheduleDeleteFile" table="tbl_record_delete_files">
		<id name="id" type="long">
			<column name="id" not-null="true" unique="true" />
			<generator class="identity" />
		</id>
		<property name="fileName" column="file_name" type="string" />
		<property name="deleteAt" column="delete_at" type="timestamp" />
	</class>
</hibernate-mapping>
