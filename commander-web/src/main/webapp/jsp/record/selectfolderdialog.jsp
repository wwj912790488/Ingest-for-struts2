<div id="dialg_for_select_folder" style="display: none;background: white">
	<div class="template_title">
		<s:text name="common.select.folder"/>
	</div>
	<div class="template_content">
		<div id="fileTreeContent" style="border: 1px solid lightgray; margin: 20px 0px; padding-left: 5px; height: 300px; overflow: auto">
		</div>
	</div>
	<div class="template_button">
		<div class="div_space"></div>
		<div class="div_buttons">
			<table align="center">
			<tr>
				<td><input type="button" id="btnOk" value="<s:text name='msg.ok'/>" /></td>
				<td class="td_space"></td>
				<td><input type="button" id="btnRefresh" value="<s:text name='common.action.refresh'/>" /></td>
				<td class="td_space"></td>
				<td><input type="button"id="btnCancel" value="<s:text name='msg.cancel'/>" /></td>
			<tr>
			</table>
		</div>
	</div>
</div>

