<?xml version="1.0" encoding="UTF-8"?>
<schedule>
	<name></name>
	<encodingoption>Custom</encodingoption>
	<priority>5</priority>
	<globalcodecsetting>
		<fillonstreamlost>1</fillonstreamlost>
	</globalcodecsetting>
	<streams>
		<stream id = "-1">
			<h264>
				<profile>Main</profile>
				<width>640</width>
				<height>480</height>
				<par>source</par>
				<par_x>-1</par_x>
				<par_y>-1</par_y>
				<fr>source</fr>
				<fr_x>-1</fr_x>
				<fr_y>-1</fr_y>
				<interpolate>1</interpolate>
				<rc>VBR</rc>
				<bitrate>1000</bitrate>
				<maxbitrate>3000</maxbitrate>
				<cqquantizer>40</cqquantizer>
				<transform8x8>0</transform8x8>
				<intraprediction8x8>0</intraprediction8x8>				
				<bufsize>375</bufsize>
				<buffillpct>1000</buffillpct>
				<gopsize>30</gopsize>
				<scd>2</scd>
				<bframe>0</bframe>
				<cabac>1</cabac>
				<loopfilter>1</loopfilter>
				<refframe>1</refframe>
				<interlace>-1</interlace>
				<topfieldfirst>-1</topfieldfirst>
				<smartborder>1</smartborder>
				<twopass>0</twopass>
				<deviceid>0</deviceid>
				<qualitylevel>2</qualitylevel>
				<simhd>
	                <deblock>0</deblock>
	                <deinterlace>2</deinterlace>
	                <denoise>0</denoise>
	                <denoisemethod>0</denoisemethod>
	                <delight>0</delight>
	                <sharpen>0</sharpen>
	                <antialias>0</antialias>
	                <antishaking>-1</antishaking>
	                <brightness>0</brightness>
	                <contrast>0</contrast>
	                <saturation>0</saturation>
	                <hue>0</hue>
	            </simhd>
			</h264>
			<aac>
				<profile>LC</profile>
				<channel>2</channel>
				<bitrate>64000</bitrate>
				<samplerate>32000</samplerate>
				<volumemode>0</volumemode>
				<balancedb>-30</balancedb>
				<balancelevel>0</balancelevel>
				<boostlevel>0</boostlevel>
			</aac>			
		</stream>
	</streams>
	<inputs>
		<localfile>
			<uri></uri>
			<preprocessor>
				<cropping>
					<enabled>0</enabled>
					<top>0</top>
					<left>0</left>
					<width>0</width>
					<height>0</height>
				</cropping>
				<timesliceclipping>
					<enabled>0</enabled>
					<trimmed>1</trimmed>
				</timesliceclipping>
			</preprocessor>
		</localfile>
	</inputs>
	<outputgroups>		
		<filearchive>
			<container>TS</container>
			<tssetting>
				<servername></servername>
				<serviceprovider></serviceprovider>
				<serviceid></serviceid>
				<totalbitrate></totalbitrate>
				<bitratemode>-1</bitratemode>
				<pmtpid></pmtpid>
				<videopid></videopid>
				<audiopid></audiopid>
				<pcrpid></pcrpid>
			</tssetting>
			<description></description>
			<targetname></targetname>
			<segmentname>${starttime}-${id}-${seq}</segmentname>
			<playlistname>${id}</playlistname>
			<output streamref = "-1" />
		</filearchive>
	</outputgroups>

	<!-- <starttype>RESERVED</starttype> -->
	<starttime>2015-12-12T12:00:00</starttime>
	<endtype>auto</endtype>
	<endtime>2015-12-12T13:00:00</endtime>
	<!-- <duration>RESERVED</duration> -->

	<!-- <recurrencepattern recurrence="on"> -->
	<recurrencepattern>
		<!-- <recurrencetype>daily</recurrencetype> -->
		<off></off>
