<?xml version="1.0" encoding="UTF-8"?>
<preset>
	<name>互联网流畅</name>
	<description></description>
	<category>预设</category>
	<stream>
		<h264>
			<profile>High</profile>
			<level>-1</level>
			<width>640</width>
			<height>480</height>
			<par>source</par>
			<par_x>-1</par_x>
			<par_y>-1</par_y>
			<fr>source</fr>
			<fr_x>-1</fr_x>
			<fr_y>-1</fr_y>
			<interpolate>1</interpolate>
			<frameratesourcemode>0</frameratesourcemode>
			<rc>VBR</rc>
			<bitrate>350</bitrate>
			<maxbitrate>1050</maxbitrate>
			<cqquantizer>0</cqquantizer>
			<transform8x8>1</transform8x8>
			<intraprediction8x8>1</intraprediction8x8>
			<bufsize>132</bufsize>
			<buffillpct>1005</buffillpct>
			<gopsize>250</gopsize>
			<gopmodeid>1</gopmodeid>
			<scd>0</scd>
			<bframe>0</bframe>
			<cabac>1</cabac>
			<refframe>1</refframe>
			<interlace>0</interlace>
			<threadcount>0</threadcount>
			<lookheadframe>0</lookheadframe>
			<smartborder>1</smartborder>
			<twopass>0</twopass>
			<deviceid>0</deviceid>
			<qualitylevel>2</qualitylevel>
			<simhd>
				<deblock>0</deblock>
				<deinterlace>2</deinterlace>
				<denoise>0</denoise>
				<denoisemethod>0</denoisemethod>
				<delight>0</delight>
				<sharpen>0</sharpen>
				<antialias>0</antialias>
				<antishaking>-1</antishaking>
				<brightness>0</brightness>
				<contrast>0</contrast>
				<saturation>0</saturation>
				<hue>0</hue>
				<resizealg>3</resizealg>
				<deinterlacealg>3</deinterlacealg>
			</simhd>
		</h264>
		<aac>
			<profile>LC</profile>
			<channel>2</channel>
			<bitrate>128000</bitrate>
			<samplerate>32000</samplerate>
			<denoise>0</denoise>
			<volumemode>0</volumemode>
			<balancedb>-30</balancedb>
			<balancelevel>0</balancelevel>
			<boostlevel>0</boostlevel>
			<channelprocessing>None</channelprocessing>
		</aac>
	</stream>
</preset>
