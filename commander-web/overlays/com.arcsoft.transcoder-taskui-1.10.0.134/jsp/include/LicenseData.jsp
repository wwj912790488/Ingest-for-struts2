		} else {
			value = license.DISABLED;
		}
	}
	else if(key == license.EX_DRM) {
		if(uGetMapValue(g_LicenseData, license.TIANYUDRM) == license.ENABLED
				|| uGetMapValue(g_LicenseData, license.ARCDRM) == license.ENABLED) {
			value = license.ENABLED;
		} else {
			value = license.DISABLED;
		}
	}
	
	return value;
}

