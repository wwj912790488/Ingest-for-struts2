function GetLicense(key) {
	var value = null;
	
	//value = GetLicenseSpecial(key);
	//if(value != null) return value;
	
	value = uGetMapValue(g_LicenseData, key);
	if(value != null) return value;
	
	value = GetLicenseCompose(key);
	
	return value;
}

/* can delete this function */
function GetLicenseSpecial(key) {
	var value = null;
	if(key == license.AUDIO_ENCODER_AAC) {
		var haveAudioCodec = false;
		for(var i = 0; i < g_LicenseData.length; i++) {
			var pilot = g_LicenseData[i].key;
			if(pilot == license.AUDIO_ENCODER_AAC
				|| pilot == license.AUDIO_ENCODER_MP2
				|| pilot == license.AUDIO_ENCODER_MP3
				|| pilot == license.AUDIO_ENCODER_WMA
				|| pilot == license.AUDIO_ENCODER_AMR
				|| pilot == license.AUDIO_ENCODER_AC3
				|| pilot == license.AUDIO_ENCODER_DTS
				|| pilot == license.AUDIO_ENCODER_PCM
				|| pilot == license.AUDIO_ENCODER_DDP) {
				if(g_LicenseData[i].value == license.ENABLED) {
					haveAudioCodec = true;
					break;
				}
			}
		}
		
		if(!haveAudioCodec) {
			value = license.ENABLED;
		}
	}
	return value;
}

